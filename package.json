    "@types/node": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "ts-node-dev": "^1.1.8",
    "@types/cli-spinner": "^0.2.1",
    "@types/inquirer": "^8.1.3",
    "@types/module-alias": "^2.0.1",
    "@types/node-emoji": "^1.8.1",
    "@types/pinyin": "^2.10.0",
    "@types/request": "^2.48.8"
  },
